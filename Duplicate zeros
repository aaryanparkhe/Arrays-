class Solution {
    public void duplicateZeros(int[] arr) {
        int n = arr.length;
         ArrayList<Integer> res = new ArrayList<>();
        for(int i = 0 ; i < n ; i++){
            if(arr[i] == 0){
                res.add(0);
                res.add(0);
            }
            else {
                res.add(arr[i]);
                  }
             arr[i] = res.get(i);
        }
       
      
    }
}



